{
  "name": "sector51client",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test -sm=false",
    "testResponsive": "ng test --environment responsive -c=karma.responsive.conf.js",
    "lint": "ng lint",
    "pree2e": "start \"json-server\" node ./e2e/scripts/server.js",
    "e2e": "ng e2e --proxy-config proxy.conf.e2e.json",
    "poste2e": "taskkill /fi \"WindowTitle eq json-server\"",
    "predeploy": "(del /F /S /Q ..\\sector51server\\src\\main\\resources\\static\\*) && (ng build --prod)",
    "deploy": "(xcopy /s .\\dist ..\\sector51server\\src\\main\\resources\\static)",
    "postdeploy": "git add ../sector51server/src/main/resources/static/**",
    "prefake": "start \"json-server\" node ./e2e/scripts/server.js",
    "fake": "ng serve --proxy-config proxy.conf.e2e.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/router": "^5.2.10",
    "@ng-bootstrap/ng-bootstrap": "^1.1.2",
    "@ngx-translate/core": "^9.0.1",
    "@ngx-translate/http-loader": "^2.0.0",
    "angular-split": "^1.0.0-rc.0",
    "angular2-websocket": "^0.9.5",
    "bootstrap": "^4.1.0",
    "core-js": "^2.5.5",
    "font-awesome": "^4.7.0",
    "primeng": "^5.2.7",
    "rxjs": "^5.5.10",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/language-service": "^5.2.10",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.106",
    "codelyzer": "^4.3.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "json-server": "^0.12.2",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "^2.8.3"
  }
}
